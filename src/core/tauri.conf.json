{
    "build": {
        "beforeDevCommand": "cd ui && yarn dev",
        "beforeBuildCommand": "cd ui && yarn build",
        "devPath": "http://localhost:1420",
        "distDir": "../ui/build",
        "withGlobalTauri": false
    },
    "package": {
        "productName": "Vla",
        "version": "0.1.0-next.8"
    },
    "tauri": {
        "allowlist": {
            "window": {
                "all": true
            }
        },
        "bundle": {
            "active": true,
            "targets": "msi",
            "identifier": "somfic.dev.vla",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ]
        },
        "security": {
            "csp": null
        },
        "macOSPrivateApi": true,
        "windows": [
            {
                "title": "Vla",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false,
                "transparent": true,
                "decorations": false,
                "visible": false
            }
        ]
    }
}