    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.4.2",
        "@tauri-apps/cli": "^1.5.0",
        "@tsconfig/svelte": "^5.0.0",
        "@types/node": "^20.9.4",
        "sass": "^1.69.5",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.6",
        "svelte-preprocess": "^5.0.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.0",
        "typescript": "^5.3.2",
        "vite": "^4.4.4"
    }
