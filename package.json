{
    "name": "vla",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "workspaces": [
        "src/svelvet",
        "src"
    ],
    "scripts": {
        "predev": "ts-node --esm scripts/build-csharp.ts",
        "dev": "vite",
        "prebuild": "ts-node --esm scripts/build-csharp.ts && yarn --cwd src/svelvet run build",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
