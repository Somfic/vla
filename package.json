{
    "name": "vla",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev-client": "vite",
        "predev-server": "ts-node --esm scripts/terminate-previous.ts",
        "dev-server": "dotnet run --project src-csharp/Vla",
        "dev": "concurrently --kill-others \"yarn run dev-client\" \"yarn run dev-server\"",
        "build-client": "vite build",
        "build-server": "ts-node --esm scripts/build-csharp.ts",
        "build": "yarn run build-client && yarn run build-server",
        "tauri": "tauri",
        "version": "ts-node --esm scripts/tag-version.ts",
        "prepare-release": "ts-node --esm scripts/prepare-release.ts",
        "test": "dotnet test src-csharp && ts-node --esm scripts/terminate-previous.ts && concurrently --kill-others \"yarn run dev\" \"wait-on tcp:127.0.0.1:55155 && playwright test\"",
        "report": "ts-node --esm scripts/report-tests.ts"
    },
    "dependencies": {
        "@octokit/request": "^8.1.6",
        "@tauri-apps/api": "^1.5.0",
        "execa": "^8.0.1",
        "fs-extra": "^11.1.1",
        "fuse.js": "^7.0.0",
        "octokit": "^3.1.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.40.1",
        "@sveltejs/vite-plugin-svelte": "^2.4.2",
        "@tauri-apps/cli": "^1.5.0",
        "@tsconfig/svelte": "^5.0.0",
        "@types/node": "^20.9.4",
        "concurrently": "^8.2.2",
        "sass": "^1.69.5",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.6",
        "svelte-preprocess": "^5.0.0",
        "svelvet": "^8.1.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.0",
        "typescript": "^5.3.2",
        "vite": "^4.4.4",
        "wait-on": "^7.2.0"
    }
}
